pipeline{
  agent any
  stages{
         stage('checkout'){
                            steps{
                                  git branch: 'main', url: 'https://github.com/nancygupta15/testonboardingassessment.git'
                                 }
                          }
         stage('test'){
                        steps{
                               dir('testonboardingassessment') {
                                                                 bat 'mvn clean install -Dbrowser=chrome'
                                                                }
                              }
                      }
        }
}